def gcd_iterativo(a, b):
    
    while(b):
        a, b = b, a % b
    return a

def gcd_recursivo(a, b):  
   
    if b == 0:
        return a
    else:
        return gcd_recursivo(b, a % b)


        # Função recursiva para calcular o n-ésimo termo da sequência de Fibonacci
def fibonacci_recursivo(n):
    if n <= 1:
        return n
    else:
        return fibonacci_recursivo(n-1) + fibonacci_recursivo(n-2)

# Função iterativa para gerar a sequência de Fibonacci até o n-ésimo termo
def fibonacci_iterativo(n):
    a, b = 0, 1
    sequencia = []
    for _ in range(n):
        sequencia.append(a)
        a, b = b, a + b
    return sequencia

# Exemplo de uso
numero = 10
print(f"Sequência de Fibonacci até o {numero}º termo (recursivo):")
for i in range(numero):
    print(fibonacci_recursivo(i))

print(f"\nSequência de Fibonacci até o {numero}º termo (iterativo):")
print(fibonacci_iterativo(numero))